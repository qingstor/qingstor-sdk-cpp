	{{- $isTypeLong := eq $property.Type "long" -}}
    {{- $isTypeBoolean := eq $property.Type "boolean" -}}
	{{- $isTypeBinary := eq $property.Type "binary" -}}
    
	{{- if or $isTypeString $isTypeTimestamp $isTypeInteger $isTypeLong $isTypeBoolean $isTypeBinary}}
