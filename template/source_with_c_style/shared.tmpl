	{{- range $_, $property := $properties -}}

	{{- $isTypeArray := eq $property.Type "array" -}}
	{{- $isTypeString := eq $property.Type "string" -}}
	{{- $isTypeTimestamp := eq $property.Type "timestamp" -}}
